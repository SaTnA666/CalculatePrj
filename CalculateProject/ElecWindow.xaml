<Window x:Class="CalculateProject.ElecWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculateProject"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Электродинамика" Height="450" Width="800" FontFamily="Trebuchet MS" Icon="ICOWindows.png">
    <Grid>
        <Button Name="ElecPower" Content="Сила тока" HorizontalAlignment="Left" Margin="320,247,0,0" VerticalAlignment="Top" Width="120" Height="22" FontFamily="Trebuchet MS" Click="PowerElec_Click"/>
        <Button Name="ResistanceElec" Content="Сопротивление" HorizontalAlignment="Left" Margin="320,220,0,0" VerticalAlignment="Top" Width="120" Height="22" Click="ResistanceElec_Click" FontFamily="Trebuchet MS"/>
        <Button x:Name="BackToMain" Content="Назад" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,392,0,0" Click="BackToMain_Click"/>

    </Grid>
</Window>
